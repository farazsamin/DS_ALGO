#include<iostream>
using namespace std;


struct node {
    int data;
    node * next;
};
node * head;

// Inserting data at nth position
void insert(int x,int n){
    node * temp1 = new node;
    temp1-> data = x;
    temp1 -> next = NULL;
    if(n==1){
        temp1->next = head;
        head= temp1;
        return;
    }
    node * temp2 = head;
    for(int i=0;i<n-2;i++){
        temp2 = temp2 ->next;
    }
    temp1-> next= temp2->next;
    temp2->next = temp1;
}
void print(){
    node * temp = head;
    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp = temp->next;
    }
}
int main(){
  head = NULL;
  insert(4,1);
  insert(8,2);
  insert(4,2);
  insert(9,2);
  insert(18,1);
  print();
}

/*
4
4 6 8 2
*/
