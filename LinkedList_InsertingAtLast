#include<iostream>
using namespace std;


struct node {
    int data;
    node * next;
};
node * head;

//insert at the last of the list
void insert(int x){
    node* temp = new node;
    temp-> data = x;
    temp-> next = NULL;
    if(head==NULL) head = temp;
    else {
      node* temp2 = head;
      while(temp2->next != NULL){
        temp2 = temp2->next;
      }
      temp2->next = temp;
    }
}

void print(){
    node * temp = head;
    while(temp!=NULL){
        cout<<temp->data<<" ";
        temp = temp->next;
    }
    cout<<endl;
}

int main(){
  head = NULL;
  insert(2);
  insert(3);
  insert(4);
  insert(1);
  print();


}

/*
4
4 6 8 2
*/
